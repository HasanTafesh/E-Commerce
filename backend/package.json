{
  "name": "backend-pro",
  "version": "1.0.0",
  "description": "Professional e-commerce backend using Node.js, Express, Sequelize (Postgres), and Redis.",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "lint": "eslint src",
    "test": "jest",
    "db:migrate": "sequelize-cli db:migrate",
